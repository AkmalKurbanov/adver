.float-auxiliary-panel 
  ul
    mixin float-auxiliary-panel(id,item,trigger)
      li 
        a(href=id class=trigger)
          i(class=item)
    +float-auxiliary-panel('#','icon-phone', 'callback-js')    
    +float-auxiliary-panel('#','icon-envelope', 'ask-js')    
    +float-auxiliary-panel('#','icon-mark','location-js')    
